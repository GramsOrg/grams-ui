{
  "name": "grams-ui",
  "version": "0.1.18",
  "description": "A collection of UI components used across grams applications",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "webpack --config config/webpack/webpack.prod.js",
    "build:old": "tsc",
    "build:storybook": "cross-env NODE_OPTIONS=--openssl-legacy-provider build-storybook",
    "build:all": "npm run build && npm run build:storybook && npm run prepublishOnly",
    "build:prod": "rimraf dist && npm run build:all",
    "build:prod:old": "rm -rf dist && npm run build:all",
    "build:prod:windows": "rd /s /q dist && npm run build:all",
    "test": "jest -u --config jestconfig.json",
    "generate": "node .generate_component",
    "storybook": "cross-env NODE_OPTIONS=--openssl-legacy-provider start-storybook -p 9009 -s public",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "upgrade": "npx npm-upgrade",
    "upgrade:local": "npm update grams-common",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "release:patch": "npm run build && npm version patch && npm publish",
    "release:minor": "npm run build && npm version minor && npm publish",
    "release:major": "npm run build && npm version major && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GramsOrg/grams-ui.git"
  },
  "keywords": [
    "grams",
    "ui",
    "library"
  ],
  "author": "GramsOrg",
  "license": "Apache-2.0",
  "homepage": "https://github.com/GramsOrg/grams-ui#README",
  "bugs": {
    "url": "https://github.com/GramsOrg/grams-ui/issues"
  },
  "files": [
    "dist/**/*"
  ],
  "devDependencies": {
    "@mdx-js/react": "^2.3.0",
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-controls": "^6.5.16",
    "@storybook/addon-docs": "^6.5.16",
    "@storybook/addon-storysource": "^6.5.16",
    "@storybook/react": "^6.5.16",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.4.0",
    "@types/loadable__component": "^5.13.4",
    "@types/storybook__react": "^5.2.1",
    "awesome-typescript-loader": "^5.2.1",
    "codecov": "^3.8.3",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "mini-css-extract-plugin": "^2.7.3",
    "prettier": "^2.8.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-test-renderer": "^18.2.0",
    "require-context.macro": "^1.2.2",
    "rimraf": "^4.4.0",
    "semantic-ui-css": "^2.5.0",
    "storybook-react-i18next": "^1.1.2",
    "ts-jest": "^29.0.5",
    "ts-loader": "^9.4.2",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.9.5",
    "typescript-plugin-css-modules": "^4.2.3",
    "webpack": "^5.76.1",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1",
    "webpack-merge": "^5.8.0",
    "webpack-node-externals": "^3.0.0"
  },
  "dependencies": {
    "@loadable/component": "^5.15.3",
    "i18next": "^22.4.11",
    "react-avatar": "^5.0.3",
    "react-i18next": "^12.2.0",
    "react-icons": "^4.8.0",
    "react-markdown": "^8.0.5",
    "semantic-ui-react": "^2.1.4"
  }
}
