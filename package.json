{
  "name": "grams-ui",
  "version": "0.1.11",
  "description": "A collection of UI components used across grams applications",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "jest --config jestconfig.json",
    "generate": "node .generate_component",
    "storybook": "cross-env NODE_OPTIONS=--openssl-legacy-provider start-storybook -p 9009 -s public",
    "build-storybook": "cross-env NODE_OPTIONS=--openssl-legacy-provider build-storybook -s public",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "release:patch": "npm run build && npm version patch && npm publish",
    "release:minor": "npm run build && npm version minor && npm publish",
    "release:major": "npm run build && npm version major && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GramsOrg/grams-ui.git"
  },
  "keywords": [
    "grams",
    "ui",
    "library"
  ],
  "author": "GramsOrg",
  "license": "Apache-2.0",
  "homepage": "https://github.com/GramsOrg/grams-ui#README",
  "bugs": {
    "url": "https://github.com/GramsOrg/grams-ui/issues"
  },
  "files": [
    "dist/**/*"
  ],
  "devDependencies": {
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/react": "^6.5.16",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.4.0",
    "@types/storybook__react": "^5.2.1",
    "codecov": "^3.8.3",
    "cross-env": "^7.0.3",
    "jest": "^29.4.3",
    "jest-environment-jsdom": "^29.4.3",
    "prettier": "^2.8.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-test-renderer": "^18.2.0",
    "require-context.macro": "^1.2.2",
    "ts-jest": "^29.0.5",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.9.5"
  }
}
